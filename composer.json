{
    "name": "corals/entity",
    "description": "Entity",
    "keywords": [
        "Entity"
    ],
    "homepage": "https://github.com/coralsio/entity",
    "license": "MIT",
    "authors": [
        {
            "name": "corals",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Corals\\Modules\\Entity\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "laravel/pint": "^1.0"
    },
    "require": {
        "corals/utility-rating": "@dev",
        "corals/utility-wishlist": "@dev",
        "corals/utility-category":"@dev",
        "corals/utility-tag":"@dev"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Corals\\Modules\\Entity\\EntityServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
